public class Solution {
    public string RestoreString(string s, int[] indices) {
        string str="";
        for (int i = 0 ; i< indices.Length; i++){
            for(int j=0; j< indices.Length; j++){
                if(indices[j]==i){
                    str+=s[j];
                }
            } 
        }
        return str;
    }
}